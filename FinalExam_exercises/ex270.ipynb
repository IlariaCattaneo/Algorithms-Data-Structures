    "def BST_COUNT_IN_RANGE(T,a,b):\n",
    "    count = 0\n",
    "    if T is None:\n",
    "        return 0\n",
    "    if T.key >= a and T.key <= b:\n",
    "        count += 1\n",
    "    if T.key > a:\n",
    "        count += BST_COUNT_IN_RANGE(T.left,a,b)\n",
    "    if T.key < b:\n",
    "        count += BST_COUNT_IN_RANGE(T.right,a,b)\n",
    "    return count"
   ]
  }
